.container{
    background-color: #f2f2f2;
    font-size: 26rpx;
    color: #666666;
    padding:15rpx;
}
.search image{
    width: 22rpx;
}
.search button{
    margin-bottom: 15rpx;
    border-radius: 100rpx;
    height: 70rpx;
    background: #fff;
    border-color: #ddd;
    color: #666;
    font-size: 24rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
view.wrapper{
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 26rpx;
    background-color: #fff;
    border-bottom: 1px solid #e5e5e5;
}
.wrapper i-icon{
    margin-left: 20rpx;
    margin-top: -5rpx;
}
.item{
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 180rpx;
    background-color: #fff;
}
.item i-icon{
    margin-right: 30rpx;
}
.item + .item{
    margin-top: 20rpx;
}
.left{
    display: flex;
    justify-content: flex-start;
}
.left .img image{
    width: 140rpx;
    width: 140rpx;
    border-radius: 100%;
    margin-left: 30rpx;
    margin-right: 30rpx;
}
.name{
    font-size: 32rpx;
    color: #333;
}
.tag{
    margin-top: 30rpx;
}
.tag text{
    font-size: 26rpx;
    color: #999999;
    padding: 5rpx 20rpx;
    border: 1rpx solid #999999;
}
.tag text + text{
    margin-left: 20rpx;
}